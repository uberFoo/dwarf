enum Option<T> {
    None,
    Some(T),
}

impl Option<T> {
    fn is_some(self) -> bool {
        match self {
            Option::<T>::Some(_) => true,
            Option::<T>::None => false,
        }
    }

    fn is_none(self) -> bool {
        match self {
            Option::<T>::Some(_) => false,
            Option::<T>::None => true,
        }
    }
}
