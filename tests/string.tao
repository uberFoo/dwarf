/// Test basic string stuff
fn main() -> () {
    let s = "Hello, world!ğŸ‰ğŸ’¥";
    print(s + "\n");

    // Strings are also iterable.
    for c in s {
        print(c + "\n");
    }

    // Can we get the length of a string?
    print(s.len() as string + "\n");

    // Can we index into a string?
    let len = s.len();
    chacha::assert_eq(s[len - 1], "ğŸ’¥");
    chacha::assert_eq(s[len - 2], "ğŸ‰");
    chacha::assert_eq(s[len - 9], " ");

    print(s[s.len() - 2] as string + "\n");
}
